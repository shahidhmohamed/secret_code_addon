<odoo>
  <record id="ir_cron_secret_codes_generate_job" model="ir.cron">
    <field name="name">Secret Codes: Generate Jobs</field>
    <field name="model_id" ref="model_secret_codes_generate_job"/>
    <field name="state">code</field>
    <field name="code">model.run_pending_jobs()</field>
    <field name="interval_number">1</field>
    <field name="interval_type">minutes</field>
    <field name="active">True</field>
  </record>

  <record id="ir_cron_secret_codes_frappe_sync" model="ir.cron">
    <field name="name">Secret Codes: Frappe Sync</field>
    <field name="model_id" ref="model_secret_codes"/>
    <field name="state">code</field>
    <field name="code">model.sync_frappe_secret_codes_cron()</field>
    <field name="interval_number">1</field>
    <field name="interval_type">minutes</field>
    <field name="active">True</field>
  </record>

</odoo>
